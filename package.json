{
   "engines": {
      "node": "12.0.0"
   },
   "name": "buffer.quench",
   "version": "1.0.0",
   "description": "back-end for buffer.quench ",
   "main": "./server.js",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/riki4iki/buffer.quench.git"
   },
   "scripts": {
      ".": "nodemon --watch src --watch libs ./src/server.ts",
      "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
      "build-ts": "tsc",
      "postinstall": "npm run build-ts",
      "start": "npm run serve",
      "serve": "node dist/server.js",
      "watch-node": "nodemon dist/server.js",
      "watch-ts": "tsc -w",
      "test": "jest --runInBand",
      "test-e2e": "jest ./test/integration --runInBand",
      "test-unit": "jest ./test/unit --runInBand"
   },
   "keywords": [
      "node",
      "typescript",
      "koa",
      "postgres",
      "typeORM"
   ],
   "author": "Ivan Aphanasyev <nekaktotak2@gmail.com>",
   "license": "ISC",
   "dependencies": {
      "@koa/cors": "^3.0.0",
      "class-validator": "^0.11.0",
      "crypto-js": "^3.1.9-1",
      "dotenv": "^8.2.0",
      "http-errors": "^1.7.3",
      "jsonwebtoken": "^8.5.1",
      "koa": "^2.11.0",
      "koa-bodyparser": "^4.2.1",
      "koa-helmet": "^5.2.0",
      "koa-logger": "^3.2.1",
      "koa-router": "^7.4.0",
      "lodash": "^4.17.15",
      "module-alias": "^2.2.2",
      "node-schedule": "^1.3.2",
      "pg": "^7.14.0",
      "pg-connection-string": "^2.1.0",
      "redis": "^3.1.1",
      "request": "^2.88.0",
      "request-promise": "^4.2.5",
      "typeorm": "^0.2.20"
   },
   "devDependencies": {
      "@types/crypto-js": "^3.1.43",
      "@types/eslint": "^6.1.7",
      "@types/http-errors": "^1.6.3",
      "@types/jest": "^24.0.25",
      "@types/jsonwebtoken": "^8.3.5",
      "@types/koa": "^2.0.52",
      "@types/koa-bodyparser": "^4.3.0",
      "@types/koa-helmet": "^3.1.2",
      "@types/koa-logger": "^3.1.1",
      "@types/koa-router": "^7.0.42",
      "@types/koa__cors": "^2.2.3",
      "@types/lodash": "^4.14.149",
      "@types/module-alias": "^2.0.0",
      "@types/node": "^12.12.14",
      "@types/node-schedule": "^1.3.0",
      "@types/redis": "^2.8.14",
      "@types/request": "^2.48.3",
      "@types/request-promise": "^4.1.45",
      "@types/supertest": "^2.0.8",
      "@typescript-eslint/eslint-plugin": "^2.18.0",
      "@typescript-eslint/parser": "^2.18.0",
      "eslint": "^6.8.0",
      "jest": "^24.9.0",
      "nodemon": "^2.0.1",
      "supertest": "^4.0.2",
      "ts-jest": "^24.3.0",
      "ts-node": "^8.5.2",
      "typescript": "^3.7.2"
   },
   "__moduleAliases": {
      "config": "src/config"
   }
}
